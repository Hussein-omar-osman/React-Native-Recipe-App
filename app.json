{
  "expo": {
    "name": "Receta",
    "slug": "receta-app",
    "description": "This is a recipe app that can help users get recipe of different meals and a YouTube video suggestion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/receta-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/firstScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
